  },
  "bin": {
    "fattennwjs": "./index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "pkg": "^5.8.1",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "fs-extra": "^11.2.0",
    "path": "^0.12.7",
    "yargs": "^17.7.2"
  }
}
