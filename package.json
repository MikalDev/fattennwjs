{
  "name": "fattennwjs",
  "version": "1.0.1",
  "description": "Fattens up a NW.js app by using an arm and intel version of the app",
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": "views/**/*",
    "targets": [
      "node18-macos-x64"
    ],
    "outputPath": "dist"
  },
  "main": "index.js",
